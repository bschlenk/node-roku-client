{
    "extends": [
        "tslint-config-airbnb"
    ],
    "rules": {
        "import-name": false,
        "max-line-length": [true, 120],
        "trailing-comma": [
            true,
            {
                "multiline": "always",
                "singleline": "never",
                "esSpecCompliant": true
            }
        ]
    }
}
