{
  "name": "roku-client",
  "version": "5.0.1",
  "description": "Send commands to your Roku devices",
  "keywords": [
    "roku",
    "ssdp"
  ],
  "homepage": "https://github.com/bschlenk/node-roku-client",
  "bugs": {
    "url": "https://github.com/bschlenk/node-roku-client/issues"
  },
  "license": "Apache-2.0",
  "author": "Brian Schlenker <bschlenk@umich.edu> (http://github.com/bschlenk)",
  "repository": {
    "type": "git",
    "url": "https://github.com/bschlenk/node-roku-client"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist",
    "!**/__tests__/"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./index.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test:unit": "jest",
    "test:watch": "npm run test:unit -- --watch",
    "lint": "eslint --ext .ts lib",
    "test": "npm run lint && npm run test:unit",
    "build": "tsc",
    "release": "standard-version",
    "prepublishOnly": "npm run test && npm run build"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "fetch-ponyfill": "^7.0.0",
    "node-ssdp": "^4.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.16",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/node": "^14.14.10",
    "@types/xml2js": "^0.4.7",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.2.1",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
