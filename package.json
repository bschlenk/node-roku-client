{
  "name": "roku-client",
  "version": "3.1.7",
  "description": "Send commands to your Roku devices",
  "engines": {
    "node": ">=6.0.0"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test:unit": "jest",
    "test:watch": "npm run test:unit -- --watch",
    "lint": "tslint -t stylish -p .",
    "test": "npm run lint && npm run test:unit",
    "build": "tsc",
    "release": "standard-version",
    "prepublishOnly": "npm run test && npm run build"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "roku",
    "ssdp"
  ],
  "bugs": {
    "url": "https://github.com/bschlenk/node-roku-client/issues"
  },
  "homepage": "https://github.com/bschlenk/node-roku-client",
  "repository": {
    "type": "git",
    "url": "https://github.com/bschlenk/node-roku-client"
  },
  "author": "Brian Schlenker <bschlenk@umich.edu> (http://github.com/bschlenk)",
  "license": "Apache-2.0",
  "dependencies": {
    "debug": "^4.1.1",
    "fetch-ponyfill": "^6.1.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.reduce": "^4.6.0",
    "lodash.values": "^4.3.0",
    "node-ssdp": "^4.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/debug": "^4.1.5",
    "@types/jest": "^24.0.25",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.reduce": "^4.6.6",
    "@types/lodash.values": "^4.3.6",
    "@types/node": "^13.1.5",
    "@types/xml2js": "^0.4.5",
    "husky": "^4.0.1",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^3.0.1",
    "prettier": "^1.19.1",
    "standard-version": "^7.0.1",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "typescript": "^3.7.4"
  }
}
