{
  "name": "roku-client",
  "version": "4.1.0",
  "description": "Send commands to your Roku devices",
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test:unit": "jest",
    "test:watch": "npm run test:unit -- --watch",
    "lint": "eslint --ext .ts lib",
    "test": "npm run lint && npm run test:unit",
    "build": "tsc",
    "release": "standard-version",
    "prepublishOnly": "npm run test && npm run build"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "roku",
    "ssdp"
  ],
  "bugs": {
    "url": "https://github.com/bschlenk/node-roku-client/issues"
  },
  "homepage": "https://github.com/bschlenk/node-roku-client",
  "repository": {
    "type": "git",
    "url": "https://github.com/bschlenk/node-roku-client"
  },
  "author": "Brian Schlenker <bschlenk@umich.edu> (http://github.com/bschlenk)",
  "license": "Apache-2.0",
  "dependencies": {
    "debug": "^4.1.1",
    "fetch-ponyfill": "^6.1.0",
    "lodash.camelcase": "^4.3.0",
    "node-ssdp": "^4.0.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/debug": "^4.1.5",
    "@types/jest": "^25.2.3",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/node": "^14.0.4",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "jest-fetch-mock": "^3.0.3",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.0",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3"
  }
}
